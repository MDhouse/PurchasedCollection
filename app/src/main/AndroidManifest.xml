<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="purchases.application.purchasescollection">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_line_style"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.App.Mint">

        <activity android:name=".products.ProductsActivity" android:theme="@style/Theme.App.Mint">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".addEditProduct.AddEditProductActivity"></activity>

        <activity android:name=".productDetail.ProductDetailActivity"></activity>
    </application>

</manifest>